(program
  (name "test-fn-two-params-42")
  (doc "Test a function with two parameters.")
  (version "0.1")

  (type ExitCode
    (alias Int32)
  )

  (fn add
    (doc "Add two integers.")
    (params (a Int32) (b Int32))
    (returns Int32)
    (body (+ a b))
  )

  (entry main
    (doc "Call add with 30 and 12 to get 42.")
    (params ())
    (returns ExitCode)
    (body (add 30 12))
  )
)
