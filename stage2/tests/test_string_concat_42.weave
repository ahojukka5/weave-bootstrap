(program
  (name "test-string-concat-42")
  (doc "Concatenate '4' and '2' and compare with \"42\"; return 42 on success.")
  (include "stage1/src/string.weave")
  (entry main
    (params)
    (returns Int32)
    (body
      (do
        (let s String (string-concat "4" "2"))
        (if-stmt (== (string-eq s "42") 1)
          (return 42)
          (return 1)
        )
      )
    )
  )
) ;; program
