(program
  (name "test-sum-range-42")
  (doc "Sum integers in a range.")
  (version "0.1")

  (type ExitCode
    (alias Int32)
  )

  (fn sum-range
    (doc "Sum integers from start to end (inclusive).")
    (params (start Int32) (end Int32))
    (returns Int32)
    (body
      (do
        (let sum Int32 0)
        (let i Int32 start)
        (while (<= i end)
          (do
            (set sum (+ sum i))
            (set i (+ i 1))
          )
        )
        (return sum)
      )
    )
  )

  (entry main
    (doc "sum-range(10, 15) = 10+11+12+13+14+15 = 75, then subtract 33.")
    (params ())
    (returns ExitCode)
    (body (- (sum-range 10 15) 33))
  )
)
