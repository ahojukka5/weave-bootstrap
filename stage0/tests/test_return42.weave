(program
  (name "return42")
  (doc "Minimal program returning 42.")
  (version "0.1")
  (type ExitCode
    (alias Int32)
  ) ;; type ExitCode
  (entry main
    (doc "Return the constant 42.")
    (params ())
    (returns ExitCode)
    (body
      (return 42)
    ) ;; body
  ) ;; entry main
) ;; program
